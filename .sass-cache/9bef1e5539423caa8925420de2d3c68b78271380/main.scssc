:@template"&#@import 'compass';
@import 'reset';
@import 'utility';
@import 'breakpoints';
@import 'grid';

/* --------------------------------------------------------------------------------------- *\
  Vars
/* --------------------------------------------------------------------------------------- */

$cream: #f2e7cc;
$red: #dc5b59;
$orange: #d27f62;
$dark: #403744;
$light-on-dark:lighten($dark,30%);
$green: #468d72;
$dark:hsl(282, 11%, 23%);

body, html {
  background:$dark;
  padding:0;
  margin:0;
  position:relative;
  -webkit-font-smoothing:antialiased;
  width:100%;
  height:100%;
  overflow-x:hidden;
  @include breakpoint(big) {
    font-size:108%;
  }
  @include breakpoint(bigger) {
    font-size:112%;
  }
  @include breakpoint(huge) {
    font-size:122%;
  }
}

.img-container {
    background-color:saturate(darken($dark,5%),5%);
    position:relative;
    .lazyload { 
      display:block;
      background-size:cover;
      position: relative;
      height: 0;
      overflow: hidden;
      width:100%;
      height:100%;
      opacity:0;
     @include transition(opacity .15s linear);
     -webkit-backface-visibility:none;
     -webkit-transform: translateZ(0); // kill the flash on load
    }
}

.loaded .lazyload.fade { opacity:1; } 

.ratio-16-9 {padding-bottom:56.25%; /* 9/16*100 */ }
.ratio-4-3 {padding-bottom:75%; /* 3/4*100 */ }
.ratio-1-1 {padding-bottom:100%; /* ... */ }




/* --------------------------------------------------------------------------------------- *\
  Layout variables
/* --------------------------------------------------------------------------------------- */

$header-height:4.6em;

$nav-width-mobile:15.5em;
$nav-width-big:19%;
$nav-width-bigger:17%;
$nav-width-huge:14%;

%page-width {
  width:100%;
  @include breakpoint(big){
    width:100%;
    margin:auto;
  }
  @include breakpoint(bigger){
    min-width:1280px;
    max-width:1920px;
    width:80%;
  }
}
/* --------------------------------------------------------------------------------------- *\
  Wrapper
/* --------------------------------------------------------------------------------------- */

.outer-wrapper {
  @include transition(left .3s  cubic-bezier(0.51,  0.01,  0.37,  0.98));
  @extend %page-width;
  padding-top:$header-height;
  position:relative;
}

@import 'audio-player';

.bg {
  height:100%;
  width:100%;
  display:block;
  position:fixed;
  opacity:0;
  pointer-events:none; // allow scroll when not over content
  background-size:cover;
  background-repeat:no-repeat;
  z-index:0;
  @include transition(opacity .2s);
  &.loaded { opacity: 0.5; }
}

.red { background:$red; }
.green { background:$green; }


a {
  // normal, hover, active, visited, focus
  @include link-colors($cream, lighten($cream,5%), darken($cream,5%), darken($cream,5%), lighten($cream,5%));
}


/* --------------------------------------------------------------------------------------- *\
  Header
/* --------------------------------------------------------------------------------------- */

header.mobile,
header.desktop {
  @extend .group;
  height:$header-height;
  overflow:hidden;
  top:0;
  left:0;
  right:0;
  bottom:0;
  position:fixed;
  z-index:100;
  .inner {
    @extend %page-width;
  }
}

header.mobile {
  @include transition(left .3s  cubic-bezier(0.51,  0.01,  0.37,  0.98));
  padding:1em;
  z-index:100;
  background:$dark;

  .menu, .info, .logo {
    width:3em;
    height:3em;
    background:blue;
    cursor:pointer;
    display:block;
    
    @include transition(opacity .3s linear .28s);
    // to fade out when in mobile states
  }
  .logo {
    margin:auto;
  }
  .info {
    float:right;
  }
}

header.desktop {
  margin-right:15px;
  .logo {
    float:left;
    width:$nav-width-big;
    height:$header-height;
    background:lighten($dark,1.5%);
    border-right:solid 1px $dark;
    @include breakpoint(bigger) { width:$nav-width-bigger;  }
    @include breakpoint(huge) { width:$nav-width-huge; }
  }

  .header-bar {
    @include background(linear-gradient(left, lighten($dark,3%), lighten($dark,1.5%)));
    border-left:solid 1px $dark;
    padding:1em;
    float:left;
    width:100 - $nav-width-big; 
    @include breakpoint(bigger) { width:100 - $nav-width-bigger;  }
    @include breakpoint(huge) { width:100 - $nav-width-huge;  }

    .audio-player { width:60%; }
  }
}


/* --------------------------------------------------------------------------------------- *\
  Main Nav
/* --------------------------------------------------------------------------------------- */

// both nav and info
nav[role="navigation"],
#info {
  position:absolute;
  width:$nav-width-mobile;
  background:darken($dark,8%);
}

nav[role="navigation"] {
  left:-$nav-width-mobile;
  padding-top:$header-height;
  border-right:solid 2px darken($dark,12%);
  position:absolute;
  top:0;
  bottom:0;
  z-index:0;

  @include breakpoint(big) {
    left:0;
    right:0;
    width:$nav-width-big;
    border:none;
    background:saturate(darken($dark,2%),1%);
  }
  @include breakpoint(bigger) { width:$nav-width-bigger; }
  @include breakpoint(huge) { width:$nav-width-huge; }
}

.main-nav-container { 
  margin:0;
  position:fixed;
  top:$header-height;
  bottom:0;
  left:0;
  width:$nav-width-mobile;
  @include breakpoint(big) {position:fixed; width:$nav-width-big * 0.95; top:auto; bottom:auto; left:auto; }
  @include breakpoint(bigger) { width:$nav-width-bigger * 0.8; }
  @include breakpoint(huge) { width:$nav-width-huge *.8; }
  // mobile position fixed stuff is conditional to data-state=nav
  .nav-list {
    @include breakpoint(big) {
      padding:1em;
    }
  }
}

.nav-level-1 {
  display:block;
  text-decoration:none;
  font-size:1.6rem;
  color:desaturate(lighten($dark,40%),4%);
  border-top:solid 1px lighten($dark, 5%);
  &:first-child{border-top:none;}
  &:last-child{ border-bottom:solid 1px lighten($dark, 5%); }

  @include breakpoint(big) {
    ul { display:none; }
    &.active { 
      ul { display:block; }
      &, & li { color:$cream; }
      padding-bottom:0.5em;
    }
  }

  .nav-title {
    padding:0.75em;
    background:darken($dark,6%);
    @include breakpoint(big){
      background:none;
      font-size:1.4rem;
    }

  }


  .text {

  }

  .icon {
    display:none; // hide until I figure out what to do here
    width:1.6rem;
    height:1.6rem;
    border-radius:50%;
    background:$light-on-dark;
    margin-bottom:0.5em;
  }


  li { 
    color:$light-on-dark;
    display:block;
    padding:0.75em;
    list-style:none; 
    font-size:1rem; 
    border-bottom:solid 1px darken($dark,11%);
    &:first-child {
      border-top:solid 1px darken($dark,11%);
      @include breakpoint(big) {
        border:none;
      }
    }
    @include breakpoint(big) {
      border:none;
    }
  }
  .nav-title, li {
    @include breakpoint(big) {
      padding:0.4rem 0;
    }
  }

}


#info {
  right:-$nav-width-mobile;
  @include box-shadow(inset 1.5em 0 1.5em 0.75em hsla(0, 0%, 0%, 0.25));
  @include breakpoint(big) { display:none; }
}


@include breakpoint-reverso(small) {
  [data-state="nav-state"] .outer-wrapper,
  [data-state="nav-state"] header.mobile {
    left:$nav-width-mobile;
    .menu, .info, .logo {
      @include transition(opacity .1s linear);
    }
    .main-nav-container {
      margin:0;
      position:fixed;
      top:$header-height;
      bottom:0;
      left:0;
      width:$nav-width-mobile;
    }
    .logo, .info { // hide them, cuz they squish up otherwise due to width:100%;
      opacity:0;
    }
  }

  [data-state="info-state"] .outer-wrapper {
    @include transform(translateX(-$nav-width-mobile));
  }
}


/* --------------------------------------------------------------------------------------- *\
  Main content
/* --------------------------------------------------------------------------------------- */

.main-content {
  position:relative;
  z-index:10;
  padding:0;
  @include breakpoint(big) {
    float:left;
    padding-left:$nav-width-big;
    width:100%;
  }
  @include breakpoint(bigger) { padding-left: $nav-width-bigger; }
  @include breakpoint(huge) { padding-left: $nav-width-huge; }
}


/* --------------------------------------------------------------------------------------- *\
  Albums
/* --------------------------------------------------------------------------------------- */

.album-container {
  @extend .group;
}

.play {
  position:absolute;
  width:100%;
  height:100%;
  background:url('../images/play.svg') no-repeat;
  background-position:50%;
  z-index:10;
  pointer-events:none;
  opacity:0;
  @include transition(opacity .15s);
}


.album-cover {
  cursor:pointer;
  // @include box-shadow(0 0 2px  $dark);
  .album-cover-img {
    @include box-shadow(inset 0 1px 1px hsla(0, 0%, 100%,0.15));
  }
}


.album-meta {
  font-size:75%;
}

.playable:hover {
    @include box-shadow(0 0 1px $dark);
    .lazyload {
      opacity:0.35;
      @include transition(opacity 0s);
    }
    .play { 
      opacity: 1; 
      @include transition(opacity 0s);
    }
}







